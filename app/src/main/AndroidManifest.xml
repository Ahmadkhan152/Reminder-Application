<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.remainder">

    <application
        android:allowBackup="true"
        android:icon="@drawable/_173002_alarm_alert_bell_internet_notice_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/_173002_alarm_alert_bell_internet_notice_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Remainder">
        <activity
            android:name=".HomeActivity"
            android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity
            android:name=".GetDateAndTimeActivity"
            android:exported="false"
            android:parentActivityName=".HomeActivity">
        </activity>
        <receiver android:name=".AlarmReceiver"></receiver>
    </application>

</manifest>